<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rajita.bullet_journal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomePage" />
        <activity android:name=".NewLogs" />
        <activity android:name=".Todo" />
        <activity android:name=".Deadlines" />
        <activity android:name=".HabitTracker" />
        <activity android:name=".Birthday" />
        <activity android:name=".Trips" />
        <activity android:name=".Library"></activity>
    </application>

</manifest>